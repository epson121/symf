<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <title>{% block title %}Welcome!{% endblock %}</title>

        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,500,500i,600,600i&display=swap" rel="stylesheet" />
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_link_tags('custom') }}
            {# <link rel="preload" href="{{ preload('/build/custom.css', { as: 'style' }) }}"> #}
        {% endblock %}
    </head>
    <body>
        <header class="header">
            {% if is_granted('ROLE_USER') %}
                <p>Email: {{ app.user.email }}</p>
            {% endif %}
        </header>
        <br><br>
        <main role="main" class="container mt-5">
            
            {% block body %}{% endblock %}
        </main>

    
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('custom') }}
    {% endblock %}
    </body>
</html>
